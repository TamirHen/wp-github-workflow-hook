{
  "name": "tamirhen/wp-github-workflow-hook",
  "version": "1.0.0",
  "type": "wordpress-plugin",
  "license": "GPLv3",
  "description": "WordPress starter theme with a modern development workflow",
  "homepage": "https://github.com/TamirHen/wp-github-workflow-hook",
  "authors": [
    {
      "name": "Tamir Hen",
      "homepage": "https://github.com/tamirhen/"
    }
  ],
  "keywords": [
    "wordpress",
    "github actions",
    "github workflow"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^7.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --ignore=index.php,vendor,resources,storage,dist --extensions=php --standard=PSR12 ."
    ]
  }
}
